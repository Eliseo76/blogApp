<% include partials/header.ejs %>


<h1>BLOG PAGE</h1>
<p>Welcome to the BLOG PAGE</p>

<%blogs.forEach(function(blog){ %>
<div>

    <h2><%= blog.title %> </h2>
    <img src="<%= blog.image %>">
    <span><%= blog.created %></span>
    <p><%= blog.body %></p>

</div>

<% }) %>
<% include partials/footer.ejs %>